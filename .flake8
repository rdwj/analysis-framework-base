[flake8]
max-line-length = 100
extend-ignore = E203, W503
per-file-ignores =
    src/analysis_framework_base/models.py:E501
    src/analysis_framework_base/exceptions.py:E501
    src/analysis_framework_base/interfaces.py:E501
exclude =
    .git,
    __pycache__,
    build,
    dist,
    venv,
    .venv,
    *.egg-info
